{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@file-generator": "./src/file-generator",
          "@file-reader": "./src/file-reader",
          "@functions": "./src/functions"
        }
      }
    ]
  ]
}